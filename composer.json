{
  "name": "group3/capybara-online-judge",
  "license": "MIT LICENSE",
  "autoload": {
    "psr-4": {
      "App\\": "src/App/",
      "Judger\\": "src/Judger/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/_files/"
    }
  },
  "authors": [
    {
      "name": "mtaku3",
      "email": "m.taku3.1222@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "twig/twig": "^3.0",
    "vlucas/phpdotenv": "^5.5",
    "firebase/php-jwt": "^6.4",
    "guzzlehttp/guzzle": "^7.5",
    "php-di/php-di": "^7.0",
    "ramsey/uuid": "^4.7",
    "cycle/orm": "^2.3",
    "spiral/pagination": "^3.7",
    "monolog/monolog": "^3.4"
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  },
  "require-dev": {
    "bartlett/umlwriter": "^3",
    "phpunit/phpunit": "^10"
  }
