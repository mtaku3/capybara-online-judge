{% set activeTab = 'testcases' %}
{% set availableLanguages = [ "C", "CPP" ] %}
{% extends "Component/ProblemContainer.twig" %}
{% block head %}
    {% include "Component/filepond.twig" %}
{% endblock head %}
{% block contents %}
    <div class="w-full relative">
        <div class="flex mb-6">
            <button type="button"
                    data-modal-target="language-edit-modal"
                    data-modal-toggle="language-edit-modal"
                    class="text-blue-700 border border-blue-700 hover:bg-blue-700 hover:text-white focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center mr-2 dark:border-blue-500 dark:text-blue-500 dark:hover:text-white dark:focus:ring-blue-800 dark:hover:bg-blue-500">
                <svg class="w-5 h-5 mr-2 -ml-1"
                     fill="none"
                     stroke="currentColor"
                     stroke-width="1.5"
                     viewBox="0 0 24 24"
                     xmlns="http://www.w3.org/2000/svg"
                     aria-hidden="true">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M3 3v1.5M3 21v-6m0 0l2.77-.693a9 9 0 016.208.682l.108.054a9 9 0 006.086.71l3.114-.732a48.524 48.524 0 01-.005-10.499l-3.11.732a9 9 0 01-6.085-.711l-.108-.054a9 9 0 00-6.208-.682L3 4.5M3 15V4.5">
                    </path>
                </svg>
                対応言語を編集
            </button>
            <div id="language-edit-modal"
                 tabindex="-1"
                 class="fixed top-0 left-0 right-0 z-50 hidden w-full p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-[calc(100%-1rem)] max-h-full">
                <div class="relative w-full max-w-7xl max-h-full">
                    <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
                        <div class="flex items-center justify-between p-5 border-b rounded-t dark:border-gray-600">
                            <h3 class="text-xl font-medium text-gray-900 dark:text-white">対応言語編集</h3>
                            <button type="button"
                                    class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm p-1.5 ml-auto inline-flex items-center dark:hover:bg-gray-600 dark:hover:text-white"
                                    data-modal-hide="language-edit-modal">
                                <svg aria-hidden="true"
                                     class="w-5 h-5"
                                     fill="currentColor"
                                     viewBox="0 0 20 20"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd">
                                    </path>
                                </svg>
                                <span class="sr-only">対応言語編集モーダルを閉じる</span>
                            </button>
                        </div>
                        <div class="p-6">
                            <ol class="flex items-center w-full text-sm font-medium text-center text-gray-500 dark:text-gray-400 sm:text-base">
                                <li class="flex md:w-full items-center whitespace-nowrap text-blue-600 dark:text-blue-500 sm:after:content-[''] after:w-full after:h-1 after:border-b after:border-gray-200 after:border-1 after:hidden sm:after:inline-block after:mx-6 xl:after:mx-10 dark:after:border-gray-700">
                                    <span class="flex items-center after:content-['/'] sm:after:hidden after:mx-2 after:text-gray-200 dark:after:text-gray-500">
                                        <svg aria-hidden="true"
                                             class="w-4 h-4 mr-2 sm:w-5 sm:h-5"
                                             fill="currentColor"
                                             viewBox="0 0 20 20"
                                             xmlns="http://www.w3.org/2000/svg">
                                            <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd">
                                            </path>
                                        </svg>
                                        コンパイルコマンド登録
                                    </span>
                                </li>
                                <li class="flex items-center whitespace-nowrap">
                                    <span class="mr-2">2</span>
                                    実行・実行比較コマンド登録
                                </li>
                            </ol>
                        </div>
                        <form action="/problem/{{ problem.getId }}/testcases/update" method="post">
                            <div class="hidden" id="language-edit-modal-languages-to-remove"></div>
                            <div id="language-edit-modal-compilerule-tab" class="hidden">
                                <div class="p-6 space-y-6">
                                    <div class="w-full flex text-center bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
                                        <ul data-tabs-toggle="#language-edit-modal-compilerule-tab-contents"
                                            role="tablist"
                                            class="w-48 text-sm font-medium text-gray-900 bg-white border-r border-gray-200 rounded-l-lg dark:bg-gray-700 dark:border-gray-600 dark:text-white">
                                            {% for language in availableLanguages %}
                                                <li role="presentation"
                                                    class="{{ loop.last ? "flex items-center w-full px-4 py-2 rounded-b-lg" : "flex items-center w-full px-4 py-2 border-b border-gray-200 rounded-t-lg dark:border-gray-600" }}">
                                                    <input {{ (problem.getCompileRules has some e => e.getLanguage.name == "#{language}") ? "checked" : "" }} type="checkbox" value="" language="{{ language }}" onchange="languageEditModal.toggleLanguage(event)" class="w-4 h-4 text-blue-600 bg-gray-100 border-gray-300 rounded focus:ring-blue-500 dark:focus:ring-blue-600 dark:ring-offset-gray-800 focus:ring-2 dark:bg-gray-700 dark:border-gray-600">
                                                    <button type="button"
                                                            data-tabs-target="#language-edit-modal-compilerule-tab-contents-{{ language }}"
                                                            role="tab"
                                                            aria-controls="language-edit-modal-compilerule-tab-contents-{{ language }}"
                                                            aria-selected="false"
                                                            class="mx-auto text-sm font-medium">{{ language }}</button>
                                                </li>
                                            {% endfor %}
                                        </ul>
                                        <div id="language-edit-modal-compilerule-tab-contents"
                                             class="p-4 flex items-center text-left w-full">
                                            {% for language in availableLanguages %}
                                                {% with { compileRule: problem.getCompileRules|filter(c => c.getLanguage.name == language)|first } %}
                                                    <div id="language-edit-modal-compilerule-tab-contents-{{ language }}" language="{{ language }}" {{ compileRule ? "compileRuleId=#{ compileRule.getId }" : "" }} role="tabpanel" aria-labelledby="{{ language }}-tab" class="hidden w-full">
                                                        <input type="text" class="hidden" id="language-edit-modal-compilerule-tab-contents-compileruleid-{{ language }}" {{ compileRule ? "name=compileRules[#{ loop.index0 }][compileRuleId]" : "" }} value="{{ compileRule ? compileRule.getId : "" }}">
                                                        <input type="text"
                                                               class="hidden"
                                                               id="language-edit-modal-compilerule-tab-contents-language-{{ language }}"
                                                               value="{{ language }}">
                                                        <div class="mb-4">
                                                            <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">ソースコードで提出時のコンパイルコマンド</label>
                                                            <input type="text" id="language-edit-modal-compilerule-tab-contents-sourcecodecompilecommand-{{ language }}" {{ compileRule ? "name=compileRules[#{ loop.index0 }][sourceCodeCompileCommand]" : "" }} value="{{ compileRule ? compileRule.getSourceCodeCompileCommand : "" }}" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                        </div>
                                                        <div>
                                                            <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">ファイルで提出時のコンパイルコマンド</label>
                                                            <input type="text" id="language-edit-modal-compilerule-tab-contents-filecompilecommand-{{ language }}" {{ compileRule ? "name=compileRules[#{ loop.index0 }][fileCompileCommand]" : "" }} value="{{ compileRule ? compileRule.getFileCompileCommand : "" }}" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                        </div>
                                                    </div>
                                                {% endwith %}
                                            {% endfor %}
                                        </div>
                                    </div>
                                </div>
                                <div class="flex items-center p-6 space-x-2 border-t border-gray-200 rounded-b dark:border-gray-600">
                                    <button id="language-edit-modal-compilerule-tab-contents-next-button"
                                            type="button"
                                            class="ml-auto first-line:text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                                        次へ
                                    </button>
                                </div>
                            </div>
                            <div id="language-edit-modal-executionrule-tab" class="hidden">
                                <div class="p-6 space-y-6">
                                    <div id="language-edit-modal-executionrule-tab-contents-notation"
                                         class="w-full flex items-center justify-center">編集は必要ありません</div>
                                    <div id="language-edit-modal-executionrule-tab-contents-edit"
                                         class="hidden w-full bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
                                        <ul class="ml-48 flex flex-wrap text-sm font-medium text-center text-gray-500 border-b border-gray-200 rounded-tr-lg bg-gray-50 dark:border-gray-700 dark:text-gray-400 dark:bg-gray-800"
                                            data-tabs-toggle="#language-edit-modal-executionrule-tab-contents"
                                            role="tablist">
                                            <li class="mr-2 flex">
                                                <button data-tabs-target="#language-edit-modal-executionrule-tab-contents-sourcecode-tab"
                                                        type="button"
                                                        role="tab"
                                                        aria-controls="sourcecode"
                                                        aria-selected="true"
                                                        class="flex-grow inline-block px-4 text-blue-600 rounded-tl-lg hover:bg-gray-100 dark:bg-gray-800 dark:hover:bg-gray-700 dark:text-blue-500">
                                                    ソースコードで提出時
                                                </button>
                                            </li>
                                            <li class="mr-2 flex">
                                                <button data-tabs-target="#language-edit-modal-executionrule-tab-contents-file-tab"
                                                        type="button"
                                                        role="tab"
                                                        aria-controls="file"
                                                        aria-selected="false"
                                                        class="flex-grow inline-block px-4 hover:text-gray-600 hover:bg-gray-100 dark:hover:bg-gray-700 dark:hover:text-gray-300">
                                                    ファイルで提出時
                                                </button>
                                            </li>
                                            <select id="language-edit-modal-executionrule-tab-contents-select" onchange="languageEditModal.changeSelectLanguage(this)" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block ml-auto mr-2 my-2 p-2 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                <option class="hidden" id="language-edit-modal-executionrule-tab-contents-option-DUMMY" language="DUMMY">DUMMY</option>
                                                {% for language in availableLanguages %}
                                                    <option id="language-edit-modal-executionrule-tab-contents-option-{{ language }}"
                                                            class="hidden"
                                                            language="{{ language }}">{{ language }}</option>
                                                {% endfor %}
                                            </select>
                                        </ul>
                                        <div class="flex w-full">
                                            <div class="w-48 text-sm font-medium text-gray-900 bg-white rounded-bl-lg dark:bg-gray-700 dark:text-white">
                                                {% for testCase in problem.getTestCases %}
                                                    {% if not loop.last %}
                                                        <button type="button" id="language-edit-modal-executionrule-tab-contents-testcase-select-{{ testCase.getId }}" class="block w-full px-4 py-2 border-b border-gray-200 cursor-pointer hover:bg-gray-100 hover:text-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-700 focus:text-blue-700 dark:border-gray-600 dark:hover:bg-gray-600 dark:hover:text-white dark:focus:ring-gray-500 dark:focus:text-white">
                                                            {{ testCase.getTitle }}
                                                        </button>
                                                    {% endif %}
                                                    {% if loop.last %}
                                                        <button type="button" id="language-edit-modal-executionrule-tab-contents-testcase-select-{{ testCase.getId }}" class="block w-full px-4 py-2 cursor-pointer hover:bg-gray-100 hover:text-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-700 focus:text-blue-700 dark:border-gray-600 dark:hover:bg-gray-600 dark:hover:text-white dark:focus:ring-gray-500 dark:focus:text-white">
                                                            {{ testCase.getTitle }}
                                                        </button>
                                                    {% endif %}
                                                {% endfor %}
                                            </div>
                                            <div id="language-edit-modal-executionrule-tab-contents" class="flex-grow">
                                                <div class="hidden p-4 bg-white rounded-lg md:p-8 dark:bg-gray-800"
                                                    id="language-edit-modal-executionrule-tab-contents-sourcecode-tab"
                                                    role="tabpanel"
                                                    aria-labelledby="sourcecode-tab">
                                                    <div id="language-edit-modal-sourcecode-tab-contents-DUMMY" language="DUMMY"></div>
                                                    {% for language in availableLanguages %}
                                                        <div id="language-edit-modal-sourcecode-tab-contents-{{ language }}"
                                                            language="{{ language }}">
                                                            {% for testCase in problem.getTestCases %}
                                                                <div id="language-edit-modal-sourcecode-tab-contents-{{ language }}-{{ testCase.getId }}"
                                                                    testCaseId="{{ testCase.getId }}"
                                                                    language="{{ language }}"
                                                                    class="hidden p-4 rounded-lg bg-white dark:bg-gray-800">
                                                                    <input id="language-edit-modal-sourcecode-tab-contents-testcaseid-{{ testCase.getId }}"
                                                                        type="text"
                                                                        value="{{ testCase.getId }}"
                                                                        class="hidden">
                                                                    <input id="language-edit-modal-sourcecode-tab-contents-language-{{ language }}"
                                                                        type="text"
                                                                        value="{{ language }}"
                                                                        class="hidden">
                                                                    <div class="mb-4">
                                                                        <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">実行コマンド</label>
                                                                        <input id="language-edit-modal-sourcecode-tab-contents-sourcecodeexecutioncommand-{{ testCase.getId }}"
                                                                            type="text"
                                                                            class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                                    </div>
                                                                    <div class="mb-4">
                                                                        <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">実行結果比較コマンド</label>
                                                                        <input id="language-edit-modal-sourcecode-tab-contents-sourcecodecomparecommand-{{ testCase.getId }}"
                                                                            type="text"
                                                                            class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                                    </div>
                                                                </div>
                                                            {% endfor %}
                                                        </div>
                                                    {% endfor %}
                                                </div>
                                                <div class="hidden p-4 bg-white rounded-lg md:p-8 dark:bg-gray-800"
                                                    id="language-edit-modal-executionrule-tab-contents-file-tab"
                                                    role="tabpanel"
                                                    aria-labelledby="file-tab">
                                                    <div id="language-edit-modal-file-tab-contents-DUMMY" language="DUMMY"></div>
                                                    {% for language in availableLanguages %}
                                                        <div id="language-edit-modal-file-tab-contents-{{ language }}"
                                                            language="{{ language }}">
                                                            {% for testCase in problem.getTestCases %}
                                                                <div id="language-edit-modal-file-tab-contents-{{ language }}-{{ testCase.getId }}"
                                                                    testCaseId="{{ testCase.getId }}"
                                                                    language="{{ language }}"
                                                                    class="hidden p-4 rounded-lg bg-white dark:bg-gray-800">
                                                                    <div class="mb-4">
                                                                        <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">実行コマンド</label>
                                                                        <input id="language-edit-modal-file-tab-contents-fileexecutioncommand-{{ testCase.getId }}"
                                                                            type="text"
                                                                            class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                                    </div>
                                                                    <div class="mb-4">
                                                                        <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">実行結果比較コマンド</label>
                                                                        <input id="language-edit-modal-file-tab-contents-filecomparecommand-{{ testCase.getId }}"
                                                                            type="text"
                                                                            class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                                                    </div>
                                                                </div>
                                                            {% endfor %}
                                                        </div>
                                                    {% endfor %}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="flex items-center p-6 space-x-2 border-t border-gray-200 rounded-b dark:border-gray-600">
                                    <button id="language-edit-modal-executionrule-tab-contents-back-button"
                                            type="button"
                                            class="text-gray-500 bg-white hover:bg-gray-100 focus:ring-4 focus:outline-none focus:ring-blue-300 rounded-lg border border-gray-200 text-sm font-medium px-5 py-2.5 hover:text-gray-900 focus:z-10 dark:bg-gray-700 dark:text-gray-300 dark:border-gray-500 dark:hover:text-white dark:hover:bg-gray-600 dark:focus:ring-gray-600">
                                        戻る
                                    </button>
                                    <button id="language-edit-modal-executionrule-tab-contents-save-button"
                                            type="submit"
                                            class="ml-auto first-line:text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                                        保存
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <button type="button"
                    data-modal-target="create-modal"
                    data-modal-toggle="create-modal"
                    class="ml-auto text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center inline-flex items-center mr-2 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800">
                <svg class="w-5 h-5 mr-2 -ml-1 stroke-black"
                     fill="currentColor"
                     viewBox="0 0 20 20"
                     xmlns="http://www.w3.org/2000/svg"
                     aria-hidden="true">
                    <path clip-rule="evenodd" fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm.75-11.25a.75.75 0 00-1.5 0v2.5h-2.5a.75.75 0 000 1.5h2.5v2.5a.75.75 0 001.5 0v-2.5h2.5a.75.75 0 000-1.5h-2.5v-2.5z">
                    </path>
                </svg>
                新規作成
            </button>
            <div id="create-modal"
                 tabindex="-1"
                 aria-hidden="true"
                 class="fixed top-0 left-0 right-0 z-50 hidden w-full p-4 overflow-x-hidden overflow-y-auto md:inset-0 h-[calc(100%-1rem)] max-h-full">
                <div class="relative w-full max-w-2xl max-h-full">
                    <div class="relative bg-white rounded-lg shadow dark:bg-gray-700">
                        <div class="flex items-start justify-between p-4 border-b rounded-t dark:border-gray-600">
                            <h3 class="text-xl font-semibold text-gray-900 dark:text-white">テストケースの新規作成</h3>
                            <button type="button"
                                    class="text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-900 rounded-lg text-sm p-1.5 ml-auto inline-flex items-center dark:hover:bg-gray-600 dark:hover:text-white"
                                    data-modal-hide="create-modal">
                                <svg aria-hidden="true"
                                     class="w-5 h-5"
                                     fill="currentColor"
                                     viewBox="0 0 20 20"
                                     xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd">
                                    </path>
                                </svg>
                                <span class="sr-only">テストケース作成モーダルを閉じる</span>
                            </button>
                        </div>
                        <form action="/problem/{{ problemId }}/testcase" method="post" enctype="multipart/form-data">
                            <div class="p-6 space-y-6 text-left">
                                <div>
                                    <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">ケース名</label>
                                    <input type="text"
                                           name="title"
                                           class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500">
                                </div>
                                <div>
                                    <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">入力データ</label>
                                    <input id="create-modal-inputFile" type="file" name="inputFile">
                                    <label class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">出力データ</label>
                                    <input id="create-modal-outputFile" type="file" name="outputFile">
                                </div>
                                <div class="w-full bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
                                    <ul class="flex flex-wrap text-sm font-medium text-center text-gray-500 border-b border-gray-200 rounded-t-lg bg-gray-50 dark:border-gray-700 dark:text-gray-400 dark:bg-gray-800"
                                        data-tabs-toggle="#create-modal-tab-contents"
                                        role="tablist">
                                        <li class="mr-2 flex">
                                            <button data-tabs-target="#create-modal-sourcecode-tab"
                                                    type="button"
                                                    role="tab"
                                                    aria-controls="sourcecode"
                                                    aria-selected="true"
                                                    class="flex-grow inline-block px-4 text-blue-600 rounded-tl-lg hover:bg-gray-100 dark:bg-gray-800 dark:hover:bg-gray-700 dark:text-blue-500">
                                                ソースコードで提出時
                                            </button>
                                        </li>
                                        <li class="mr-2 flex">
                                            <button data-tabs-target="#create-modal-file-tab"
                                                    type="button"
                                                    role="tab"
                                                    aria-controls="file"
                                                    aria-selected="false"
                                                    class="flex-grow inline-block px-4 hover:text-gray-600 hover:bg-gray-100 dark:hover:bg-gray-700 dark:hover:text-gray-300">
                                                ファイルで提出時
                                            </button>
                                        </li>
                                        <select onchange="changeCreateModalSelectLanguage(this)"
