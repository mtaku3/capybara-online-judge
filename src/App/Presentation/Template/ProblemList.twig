            const editModalFileTabElements = document.querySelectorAll("div[id^=edit-modal-tab-contents] > div[id^=edit-modal-file-tab]");

            const editModalTabs = {};
            const editModaSavelEl = document.querySelector("#edit-modal-save");
            editModaSavelEl.addEventListener("onclick", () => {
                for (let tab of editModalSourceCodeTabElements) {
                    const testCaseId = tab.getAttribute("testCaseId");
                    const targetElements = [...tab.querySelectorAll("div[id^=edit-modal-sourcecode-tab-contents]")];
                    editModalTabs[testCaseId] = {
                        sourcecode: new Tabs(
                            targetElements.map((el) => ({
                                id: el.getAttribute("executionRuleId"),
                                triggerEl: document.querySelector(`option[executionRuleId="${el.getAttribute("executionRuleId")}"]`),
                                targetEl: el
                            })), {
                                defaultTabId: targetElements[0].getAttribute("executionRuleId")
                            }
                        )
                    };
                }
                for (let tab of editModalFileTabElements) {
                    const testCaseId = tab.getAttribute("testCaseId");
                    const targetElements = [...tab.querySelectorAll("div[id^=edit-modal-file-tab-contents]")];
                    editModalTabs[testCaseId] = Object.assign(editModalTabs[testCaseId], {
                        file: new Tabs(
                            targetElements.map((el) => ({
                                id: el.getAttribute("executionRuleId"),
                                triggerEl: document.querySelector(`option[executionRuleId="${el.getAttribute("executionRuleId")}"]`),
                                targetEl: el
                            })), {
                                defaultTabId: targetElements[0].getAttribute("executionRuleId")
                            }
                        )
                    });
                }
            })
            

            function changeEditModalSelectLanguage(el) {
                let executionRuleId = el.selectedOptions[0].getAttribute("executionRuleId");
                let testCaseId = el.selectedOptions[0].getAttribute("testCaseId");
                editModalTabs[testCaseId].sourcecode.show(executionRuleId);
                editModalTabs[testCaseId].file.show(executionRuleId);
            }
        }
        
