    <script>
        class LanguageEditModal {
            constructor() {
                this.languages = [{{  problem.getCompileRules|map(c => "'#{c.getLanguage.name}'")|join(", ")|raw }}];
                this.languagesToAdd = [];
                this.availableLanguages = [{{ availableLanguages|map(x => "'#{x}'")|join(", ")|raw }}];
                this.languagesToRemoveEl = document.querySelector("#language-edit-modal-languages-to-remove");

                this.stepperTab = new Tabs([{
                    id: "compilerule",
                    triggerEl: document.querySelector("#language-edit-modal-executionrule-tab-contents-back-button"),
                    targetEl: document.querySelector("#language-edit-modal-compilerule-tab"),
                }, {
                    id: "executionrule",
                    triggerEl: document.querySelector("#language-edit-modal-compilerule-tab-contents-next-button"),
                    targetEl: document.querySelector("#language-edit-modal-executionrule-tab"),
                }]);

                let sourceCodeTabContentsElementsBylanguage = [...document.querySelectorAll("div[id='language-edit-modal-executionrule-tab-contents-sourcecode-tab'] > div[id^='language-edit-modal-sourcecode-tab-contents']")];
                let fileTabContentsElementsBylanguage = [...document.querySelectorAll("div[id='language-edit-modal-executionrule-tab-contents-file-tab'] > div[id^='language-edit-modal-file-tab-contents']")];

                this.executionRuleTabs = {
                    language: {
                        sourcecode: new Tabs(sourceCodeTabContentsElementsBylanguage.map(el => ({
                            id: el.getAttribute("language"),
                            triggerEl: document.querySelector(`option[id='language-edit-modal-executionrule-tab-contents-option-${el.getAttribute("language")}']`),
                            targetEl: el
                        }))),
                        file: new Tabs(fileTabContentsElementsBylanguage.map(el => ({
                            id: el.getAttribute("language"),
                            triggerEl: document.querySelector(`option[id='language-edit-modal-executionrule-tab-contents-option-${el.getAttribute("language")}']`),
                            targetEl: el
                        })))
                    },
                    testCase: {},
                    index: 0
                };
                for (let el of sourceCodeTabContentsElementsBylanguage) {
                    let language = el.getAttribute("language");
                    let sourceCodeTargetElements = [...el.querySelectorAll(`div[id^='language-edit-modal-sourcecode-tab-contents-${language}']`)];
                    let fileTargetElements = [...document.querySelectorAll(`div[id='language-edit-modal-executionrule-tab-contents-file-tab'] > div[id='language-edit-modal-file-tab-contents-${language}'] > div[id^='language-edit-modal-file-tab-contents-${language}-']`)];
                    this.executionRuleTabs.testCase[language] = {
                        sourcecode: new Tabs(sourceCodeTargetElements.map(targetEl => ({
                            id: `${language}-${targetEl.getAttribute("testCaseId")}`,
                            triggerEl: document.querySelector(`button[id='language-edit-modal-executionrule-tab-contents-testcase-select-${targetEl.getAttribute("testCaseId")}']`),
                            targetEl: targetEl,
                        }))),
                        file: new Tabs(fileTargetElements.map(targetEl => ({
                            id: `${language}-${targetEl.getAttribute("testCaseId")}`,
                            triggerEl: document.querySelector(`button[id='language-edit-modal-executionrule-tab-contents-testcase-select-${targetEl.getAttribute("testCaseId")}']`),
                            targetEl: targetEl,
                        })))
                    };
                }
            }

            changeSelectLanguage(el) {
                let language = el.selectedOptions[0].getAttribute("language");
                this.executionRuleTabs.language.sourcecode.show(language);
                this.executionRuleTabs.language.file.show(language);
            }

            toggleLanguage(e) {
                let language = e.target.getAttribute("language");
                let checked = e.target.checked;
                let compileRuleIdx = this.availableLanguages.indexOf(language);

                if (this.languages.includes(language)) {

                    if (!checked) {
                        let input = document.createElement("input");
                        input.classList.add("hidden");
                        input.setAttribute("type", "text");
                        input.setAttribute("name", `languagesToRemove[${compileRuleIdx}]`);
                        input.setAttribute("value", language);
                        this.languagesToRemoveEl.appendChild(input);

                        let tabContentsEl = document.querySelector(`div[id='language-edit-modal-compilerule-tab-contents-${language}']`);
                        let compileRuleIdEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-compileruleid']");
                        let sourceCodeCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-sourcecodecompilecommand']");
                        let fileCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-filecompilecommand']");

                        compileRuleIdEl.removeAttribute("name");
                        sourceCodeCompileCommandEl.removeAttribute("name");
                        fileCompileCommandEl.removeAttribute("name");
                    } else {
                        let input = this.languagesToRemoveEl.querySelector(`input[value=${language}]`);
                        if (!input) throw new Error(`Unable to find the input element with name ${language}`);
                        this.languagesToRemoveEl.removeChild(input);

                        let tabContentsEl = document.querySelector(`div[id='language-edit-modal-compilerule-tab-contents-${language}']`);
                        let compileRuleIdEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-compileruleid']");
                        let sourceCodeCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-sourcecodecompilecommand']");
                        let fileCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-filecompilecommand']");

                        compileRuleIdEl.setAttribute("name", `compileRules[${compileRuleIdx}][compileRuleId]`);
                        sourceCodeCompileCommandEl.setAttribute("name", `compileRules[${compileRuleIdx}][sourceCodeCompileCommand]`);
                        fileCompileCommandEl.setAttribute("name", `compileRules[${compileRuleIdx}][fileCompileCommand]`);
                    }
                } else {
                    let executionRuleIdx = this.executionRuleTabs.index;
                    
                    if (checked) {
                        let tabContentsEl = document.querySelector(`div[id='language-edit-modal-compilerule-tab-contents-${language}']`);
                        let languageEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-language']");
                        let sourceCodeCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-sourcecodecompilecommand']");
                        let fileCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-filecompilecommand']");

                        languageEl.setAttribute("name", `compileRules[${compileRuleIdx}][language]`);
                        sourceCodeCompileCommandEl.setAttribute("name", `compileRules[${compileRuleIdx}][sourceCodeCompileCommand]`);
                        fileCompileCommandEl.setAttribute("name", `compileRules[${compileRuleIdx}][fileCompileCommand]`);

                        if (this.languagesToAdd.length == 0) {
                            let notationEl = document.querySelector("#language-edit-modal-executionrule-tab-contents-notation");
                            notationEl.classList.add("hidden");

                            let editEl = document.querySelector("#language-edit-modal-executionrule-tab-contents-edit");
                            editEl.classList.remove("hidden");
                        }

                        let sourceCodeTabElements = document.querySelectorAll(`div[id^='language-edit-modal-sourcecode-tab-contents-${language}'] > div[id^='language-edit-modal-sourcecode-tab-contents-${language}']`);
                        for (let el of sourceCodeTabElements) {
                            let testCaseId = el.getAttribute("testCaseId");
                            let fileTabEl = document.querySelector(`div[id='language-edit-modal-file-tab-contents-${language}-${testCaseId}']`);

                            let testCaseIdEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-testcaseid']");
                            let languageEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-language']");
                            let sourceCodeExecutionCommandEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-sourcecodeexecutioncommand']");
                            let sourceCodeCompareCommandEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-sourcecodecomparecommand']");

                            let fileExecutionCommandEl = fileTabEl.querySelector("input[id^='language-edit-modal-file-tab-contents-fileexecutioncommand']");
                            let fileCompareCommandEl = fileTabEl.querySelector("input[id^='language-edit-modal-file-tab-contents-filecomparecommand']");

                            testCaseIdEl.setAttribute("name", `executionRules[${executionRuleIdx}][testCaseId]`);
                            languageEl.setAttribute("name", `executionRules[${executionRuleIdx}][language]`);
                            sourceCodeExecutionCommandEl.setAttribute("name", `executionRules[${executionRuleIdx}][sourceCodeExecutionCommand]`);
                            sourceCodeCompareCommandEl.setAttribute("name", `executionRules[${executionRuleIdx}][sourceCodeCompareCommand]`);

                            fileExecutionCommandEl.setAttribute("name", `executionRules[${executionRuleIdx}][fileExecutionCommand]`);
                            fileCompareCommandEl.setAttribute("name", `executionRules[${executionRuleIdx}][fileCompareCommand]`);

                            executionRuleIdx += 1;
                        }

                        let optionEl = document.querySelector(`option[id='language-edit-modal-executionrule-tab-contents-option-${language}']`);
                        optionEl.classList.remove("hidden");

                        if (this.languagesToAdd.length == 0) {
                            let selectEl = document.querySelector("select[id='language-edit-modal-executionrule-tab-contents-select']");
                            selectEl.value = language;
                            selectEl.dispatchEvent(new Event("change"));
                        }

                        this.languagesToAdd.push(language);

                        this.executionRuleTabs.index = executionRuleIdx;
                    } else {
                        let tabContentsEl = document.querySelector(`div[id='language-edit-modal-compilerule-tab-contents-${language}']`);
                        let languageEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-language']");
                        let sourceCodeCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-sourcecodecompilecommand']");
                        let fileCompileCommandEl = tabContentsEl.querySelector("input[id^='language-edit-modal-compilerule-tab-contents-filecompilecommand']");

                        languageEl.removeAttribute("name");
                        sourceCodeCompileCommandEl.removeAttribute("name");
                        fileCompileCommandEl.removeAttribute("name");

                        if (this.languagesToAdd.length == 1) {
                            let notationEl = document.querySelector("#language-edit-modal-executionrule-tab-contents-notation");
                            notationEl.classList.remove("hidden");

                            let editEl = document.querySelector("#language-edit-modal-executionrule-tab-contents-edit");
                            editEl.classList.add("hidden");
                        }

                        let sourceCodeTabElements = document.querySelectorAll(`div[id^='language-edit-modal-sourcecode-tab-contents-${language}'] > div[id^='language-edit-modal-sourcecode-tab-contents-${language}']`);
                        for (let el of sourceCodeTabElements) {
                            let testCaseId = el.getAttribute("testCaseId");
                            let fileTabEl = document.querySelector(`div[id='language-edit-modal-file-tab-contents-${language}-${testCaseId}']`);

                            let testCaseIdEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-testcaseid']");
                            let languageEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-language']");
                            let sourceCodeExecutionCommandEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-sourcecodeexecutioncommand']");
                            let sourceCodeCompareCommandEl = el.querySelector("input[id^='language-edit-modal-sourcecode-tab-contents-sourcecodecomparecommand']");

                            let fileExecutionCommandEl = fileTabEl.querySelector("input[id^='language-edit-modal-file-tab-contents-fileexecutioncommand']");
                            let fileCompareCommandEl = fileTabEl.querySelector("input[id^='language-edit-modal-file-tab-contents-filecomparecommand']");

                            testCaseIdEl.removeAttribute("name");
                            languageEl.removeAttribute("name");
                            sourceCodeExecutionCommandEl.removeAttribute("name");
                            sourceCodeCompareCommandEl.removeAttribute("name");

                            fileExecutionCommandEl.removeAttribute("name");
                            fileCompareCommandEl.removeAttribute("name");
                        }

                        let optionEl = document.querySelector(`option[id='language-edit-modal-executionrule-tab-contents-option-${language}']`);
                        optionEl.classList.add("hidden");

                        if (this.languagesToAdd.length == 1) {
                            let selectEl = document.querySelector("select[id='language-edit-modal-executionrule-tab-contents-select']");
                            selectEl.value = "DUMMY";
                            selectEl.dispatchEvent(new Event("change"));
                        }

                        this.languagesToAdd = this.languagesToAdd.filter(x => x != language);
                    }
                }
            }
        }

        const languageEditModal = new LanguageEditModal();
    </script>
    <script>
        const inputFileEl = document.querySelector("#create-modal-inputFile");
        FilePond.create(inputFileEl, {
            storeAsFile: true
        });

        const outputFileEl = document.querySelector("#create-modal-outputFile");
        FilePond.create(outputFileEl, {
            storeAsFile: true
        });

        const createModalSourceCodeTabContentsElements = [...document.querySelectorAll("div[id^=create-modal-sourcecode-tab-contents]")];
        const createModalFileTabContentsElements = [...document.querySelectorAll("div[id^=create-modal-file-tab-contents]")];

        const createModalTab = {
            sourcecode: new Tabs(
                createModalSourceCodeTabContentsElements.map((el) => ({
                    id: el.getAttribute("language"),
                    triggerEl: document.querySelector(`option[language="${el.getAttribute("language")}"]`),
                    targetEl: el
                })), {
                    defaultTabId: createModalSourceCodeTabContentsElements[0].getAttribute("language")
                }
            ),
            file: new Tabs(
                createModalFileTabContentsElements.map((el) => ({
                    id: el.getAttribute("language"),
                    triggerEl: document.querySelector(`option[language="${el.getAttribute("language")}"]`),
                    targetEl: el
                })), {
                    defaultTabId: createModalFileTabContentsElements[0].getAttribute("language")
                }
            )
        };

        function changeCreateModalSelectLanguage(el) {
            let language = el.selectedOptions[0].getAttribute("language");
            createModalTab.sourcecode.show(language);
            createModalTab.file.show(language);
        }
    </script>
    <script>
        const inputFileElements = [...document.querySelectorAll("input[id^='duplicate-modal-inputFile-']")];
        for (let el of inputFileElements) {
            FilePond.create(el, {
                storeAsFile: true
            });
        }

        const outputFileElements = [...document.querySelectorAll("input[id^='duplicate-modal-outputFile-']")];
        for (let el of outputFileElements) {
            FilePond.create(el, {
                storeAsFile: true
            });
        }

        const duplicateModalSourceCodeTabElements = document.querySelectorAll("div[id^=duplicate-modal-tab-contents] > div[id^=duplicate-modal-sourcecode-tab]");
        const duplicateModalFileTabElements = document.querySelectorAll("div[id^=duplicate-modal-tab-contents] > div[id^=duplicate-modal-file-tab]");

        const duplicateModalTabs = {};
        for (let tab of duplicateModalSourceCodeTabElements) {
            const testCaseId = tab.getAttribute("testCaseId");
            const targetElements = [...tab.querySelectorAll("div[id^=duplicate-modal-sourcecode-tab-contents]")];
            duplicateModalTabs[testCaseId] = {
                sourcecode: new Tabs(
                    targetElements.map((el) => ({
                        id: el.getAttribute("executionRuleId"),
                        triggerEl: document.querySelector(`option[executionRuleId="${el.getAttribute("executionRuleId")}"]`),
                        targetEl: el
                    })), {
                        defaultTabId: targetElements[0].getAttribute("executionRuleId")
                    }
                )
            };
        }
        for (let tab of duplicateModalFileTabElements) {
            const testCaseId = tab.getAttribute("testCaseId");
            const targetElements = [...tab.querySelectorAll("div[id^=duplicate-modal-file-tab-contents]")];
            duplicateModalTabs[testCaseId] = Object.assign(duplicateModalTabs[testCaseId], {
                file: new Tabs(
                    targetElements.map((el) => ({
                        id: el.getAttribute("executionRuleId"),
                        triggerEl: document.querySelector(`option[executionRuleId="${el.getAttribute("executionRuleId")}"]`),
                        targetEl: el
                    })), {
                        defaultTabId: targetElements[0].getAttribute("executionRuleId")
                    }
                )
            });
        }

        function changeDuplicateModalSelectLanguage(el) {
            let executionRuleId = el.selectedOptions[0].getAttribute("executionRuleId");
            let testCaseId = el.selectedOptions[0].getAttribute("testCaseId");
            duplicateModalTabs[testCaseId].sourcecode.show(executionRuleId);
            duplicateModalTabs[testCaseId].file.show(executionRuleId);
        }
